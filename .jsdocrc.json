{
  "opts": {
    "destination": "docs",
    "template": "node_modules/docdash",
    "recurse": true
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "readme.md", 
      "src", 
      "index.js", 
      "node_modules/westures-core/src",
      "node_modules/westures-core/index.js"
    ],
    "includePattern": ".js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "collapse": true,
    "search": true,
    "sectionOrder": [
      "Modules",
      "Classes"
    ],
    "sort": false,
    "typedefs": true
  }
}

